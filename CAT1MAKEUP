#include <iostream>
#include <string>
using namespace std;
class Child {
    std::string first_name;
    std::string middle_name;
public:
Child(std::string f_name, std::string m_name)
: first_name(f_name), middle_name(m_name){}

std::string getName(){
     return first_name+" "+middle_name;//TRY AND IMPLEMENT WITHOUT THE RETURN TYPE
    } //!HINT: You need to implement this function
};
//!HINT: do not change anything in main() function below
int main(){
Child child("Judah", "Ben-Hur");
cout << "They have a child named " << child.getName()
<< "!" << endl;
 return 0;
 }
